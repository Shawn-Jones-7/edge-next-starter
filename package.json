{
  "name": "cloudflare-worker-template",
  "version": "1.1.0",
  "description": "Next.js on Cloudflare Pages with Edge Runtime, D1, R2, and KV",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "prisma generate",
    "prepare": "lefthook install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit --project tsconfig.json",
    "type-check:tests": "tsc --noEmit --project tsconfig.test.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "arch:check": "depcruise --config .dependency-cruiser.cjs app components lib services repositories",
    "circular:check": "madge --circular --extensions ts,tsx,js,jsx app/ components/ lib/ services/ repositories/",
    "unused:check": "knip",
    "security:audit": "pnpm --registry https://registry.npmjs.org/ audit --audit-level moderate",
    "security:semgrep": "node scripts/security-semgrep.js",
    "security:check": "pnpm security:audit && pnpm security:semgrep",
    "perf:lighthouse": "lhci autorun --config=lighthouserc.js",
    "analyze:size": "du -sh .next/static && find .next/static -name '*.js' -exec du -h {} + | sort -rh | head -20",
    "hooks:install": "lefthook install",
    "hooks:uninstall": "lefthook uninstall",
    "pages:build": "pnpm dlx @cloudflare/next-on-pages",
    "pages:deploy": "pnpm run pages:build && wrangler pages deploy",
    "pages:deploy:test": "cp wrangler.test.toml wrangler.toml && pnpm run pages:build && wrangler pages deploy",
    "pages:deploy:prod": "cp wrangler.prod.toml wrangler.toml && pnpm run pages:build && wrangler pages deploy",
    "wrangler": "wrangler",
    "cf:dev": "wrangler pages dev .vercel/output/static",
    "db:create:test": "wrangler d1 create cloudflare-worker-template-test",
    "db:create:prod": "wrangler d1 create cloudflare-worker-template-prod",
    "db:migrations:create": "wrangler d1 migrations create cloudflare-worker-template-local",
    "db:migrations:list": "wrangler d1 migrations list cloudflare-worker-template-local --local",
    "db:migrate:local": "wrangler d1 migrations apply cloudflare-worker-template-local --local",
    "db:migrate:test": "wrangler d1 migrations apply cloudflare-worker-template-test --remote --config wrangler.test.toml",
    "db:migrate:prod": "wrangler d1 migrations apply cloudflare-worker-template-prod --remote --config wrangler.prod.toml",
    "db:validate": "node scripts/validate-migrations.js",
    "db:seed": "node scripts/seed.js",
    "prisma:generate": "prisma generate",
    "r2:create:test": "wrangler r2 bucket create cloudflare-worker-template-test",
    "r2:create:prod": "wrangler r2 bucket create cloudflare-worker-template-prod",
    "kv:create:test": "wrangler kv namespace create KV --env test",
    "kv:create:prod": "wrangler kv namespace create KV --env production"
  },
  "keywords": [
    "next.js",
    "cloudflare",
    "edge",
    "d1",
    "r2",
    "workers"
  ],
  "author": "",
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": "prettier --write"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "pnpm": {
    "overrides": {
      "esbuild": "0.25.0"
    }
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-d1": "^6.19.0",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.8",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.548.0",
    "next": "15.4.7",
    "next-intl": "^4.5.5",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-day-picker": "^9.11.2",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.66.1",
    "react-resizable-panels": "^3.0.6",
    "recharts": "2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@cloudflare/workers-types": "^4.20251121.0",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^16.0.3",
    "@next/eslint-plugin-next": "15.1.8",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.19.25",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/browser-playwright": "^4.0.13",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.13",
    "autoprefixer": "^10.4.22",
    "dependency-cruiser": "^17.3.1",
    "eslint": "^9.39.1",
    "eslint-config-next": "15.5.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-you-might-not-need-an-effect": "^0.7.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-security-node": "^1.1.4",
    "happy-dom": "^20.0.10",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "knip": "^5.70.1",
    "lefthook": "^2.0.4",
    "lint-staged": "^15.5.2",
    "madge": "^8.0.0",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "prisma": "^6.19.0",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13",
    "wrangler": "^4.50.0"
  }
}
